package eu.reitmayer.schemagen.lang;

public class SchemaLink {

	public String id;
	
	public String leftSideEntityId = "";
	
	public String rightSideEntityId = "";
	
	public String leftSideCardinalty = "";
	
	public String rightSideCardinality = "";
	
	public String navigateDirection = ""; // can be either both or left to right


	public SchemaLink(String id, String leftSideEntityId,
			String rightSideEntityId, String leftSideCardinalty,
			String rightSideCardinality, String navigateDirection) {
		super();
		this.id = id;
		this.leftSideEntityId = leftSideEntityId;
		this.rightSideEntityId = rightSideEntityId;
		this.leftSideCardinalty = leftSideCardinalty;
		this.rightSideCardinality = rightSideCardinality;
		this.navigateDirection = navigateDirection;
	}


	public SchemaLink() {
		super();
	}
	
	
}
